{
    "extends": ["airbnb" , "prettier" , "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier" : ["error", {"singleQuote": true, "parser": "flow"}],
        "no-unused-vars" :"warn",
        "no-console":"off",
        "func-names":"off",
        "no-process-exit":"off",
        "object-shorthand":"off"
    }
}
